{
    "name": "fabioguin/livewire-searchable-select",
    "description": "High-performance Livewire component for searchable select inputs with relevance-based ordering, caching, and optimized UX",
    "version": "2.0.0",
    "keywords": [
        "laravel",
        "livewire",
        "select",
        "searchable",
        "autocomplete",
        "debouncing",
        "caching",
        "relevance",
        "performance",
        "ux",
        "component"
    ],
    "homepage": "https://github.com/fabioguin/livewire-searchable-select",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fabio Guin",
            "email": "info@starzero.it",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@starzero.it",
        "issues": "https://github.com/fabioguin/livewire-searchable-select/issues"
    },
    "require": {
        "php": "^8.1|^8.2",
        "livewire/livewire": "^3.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "FabioGuin\\LivewireSearchableSelect\\": "src",
            "FabioGuin\\LivewireSearchableSelect\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FabioGuin\\LivewireSearchableSelect\\LivewireSearchableSelectServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.21.0|^9.0",
        "laravel/pint": "^1.16",
        "phpunit/phpunit": "^10.1"
    }
}
