{
    "name": "fabioguin/livewire-searchable-select",
    "description": "Livewire component for searchable select inputs",
    "keywords": ["laravel", "tailwind", "livewire", "select input", "component", "search", "input", "select2", "searchable"],
    "homepage": "https://github.com/fabioguin/livewire-select",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fabio Guin",
            "email": "info@starzero.it",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@starzero.it",
        "issues": "https://github.com/fabioguin/livewire-searchable-select/issues"
    },
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^3.0",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FabioGuin\\LivewireSelect\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FabioGuin\\LivewireSelect\\LivewireSearchableSelectServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "laravel/pint": "^1.16"
    }
}
